pipeline{
 agent any
 stages{
   stage('prepare'){
    steps{
	   git branch: 'main', url: 'https://github.com/Dhanaraj2512/quick-sync.git'
	 }
   }
   stage('build'){
     steps{
	   sh 'mvn clean install'
	 }
   }
   stage('deploy-dev'){
     steps{
	   echo "deploying an app in devo"
	 }
   }
   stage('deploy-stage'){
     steps{
	   input 'give an approval'
	   echo "deploying an app in stage"
	 }
   }
   stage('build'){
     steps{
	   sh 'mvn clean install'
	 }
   }
}
